{
  "name": "systemjs-sample-client",
  "version": "1.0.0",
  "description": "Client which use UI lib via SystemJS and Webpack",
  "scripts": {
    "dev": "webpack-dev-server --config=webpack.dev.js --open --hot",
    "build": "webpack --mode=production --config=webpack.prod.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --fix --ext .js --ext .jsx",
    "format-lint": "npm run format && npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ci": "npm run format:check && npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.2.0",
    "prettier": "^2.0.4",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
